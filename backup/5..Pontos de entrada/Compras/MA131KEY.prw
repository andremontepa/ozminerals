   cRet := 'C1_FILIAL+C1_GRADE+C1_FORNECE+C1_LOJA+C1_PRODUTO+C1_DESCRI+Dtos(C1_DATPRF)+C1_ITEM+C1_CC+C1_CONTA+C1_ITEMCTA+C1_CLVL+C1_FILENT'
