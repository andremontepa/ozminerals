user function FUNF3COM

Local cUpdate := ""
Local nStart  := 0

    cUpdate := " UPDATE " + RetSqlName("SX3")
    cUpdate += " SET X3_F3 = 'SF4' WHERE X3_CAMPO = 'D1_TES' "
    If TCSQLExec(cUpdate) < 0
    		FwLogMsg("FUNF3COM", /*cTransactionId*/, "FUNF3COM", FunName(), "", "01", "Erro ao Executar o update tabela SX3.", 0, (nStart - Seconds()), {})
    EndIf 
                       
    MATA116()

    cUpdate := " UPDATE " + RetSqlName("SX3")
